{
  "name": "harness-developer-hub",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --host 0.0.0.0",
    "build": "export NODE_OPTIONS=--max-old-space-size=4096 && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint:textlint": "export NODE_OPTIONS=--max-old-space-size=4096 && textlint tutorials/**/**.md* docs/**/**.md*",
    "lint:eslint": "eslint ./src/**/*.js*",
    "lint:vale": "npm run lint:textlint",
    "lint:ci": "export NODE_OPTIONS=--max-old-space-size=4096 && npm run lint:eslint && npm run lint:textlint",
    "test": "npm run lint:ci"
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.1",
    "@docusaurus/cssnano-preset": "^3.0.1",
    "@docusaurus/plugin-client-redirects": "^3.0.1",
    "@docusaurus/plugin-debug": "^3.0.1",
    "@docusaurus/plugin-google-analytics": "^3.0.1",
    "@docusaurus/plugin-google-gtag": "^3.0.1",
    "@docusaurus/plugin-sitemap": "^3.0.1",
    "@docusaurus/preset-classic": "^3.0.1",
    "@docusaurus/theme-classic": "^3.0.1",
    "@docusaurus/theme-mermaid": "^3.0.1",
    "@docusaurus/theme-search-algolia": "^3.0.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-plugin-sass": "^0.2.5",
    "fs-extra": "^11.2.0",
    "lottie-react": "^2.4.0",
    "prism-react-renderer": "^2.3.0",
    "rc-tooltip": "^6.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "sass": "^1.69.5",
    "update-notifier": "^7.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.0.1",
    "@tsconfig/docusaurus": "^2.0.2",
    "dictionary-en": "^4.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "textlint": "^13.4.1",
    "textlint-rule-no-todo": "^2.0.1",
    "textlint-rule-spelling": "^0.3.0",
    "textlint-rule-write-good": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
